---
export async function getStaticPaths() {
  return [
    { params: { id: '1' } },
    { params: { id: '2' } },
    { params: { id: '3' } },
  ];
}

const { id } = Astro.params;
---

<script>
// Redirect to staff detail page with proper ID handling
if (typeof window !== 'undefined') {
  const staffId = new URLSearchParams(window.location.search).get('id') || window.location.pathname.split('/').pop();
  if (staffId && staffId !== '[id]') {
    window.location.href = `/staff/index.html?id=${staffId}`;
  } else {
    window.location.href = '/staff/';
  }
}
</script>

<p>Redirecting to staff details...</p>